<?xml version="1.0" encoding="utf-8"?>
<odoo>

		<record id="mobel.product_inherit_mobel_form_view" model="ir.ui.view">
			<field name="name">mobel.inherit.product.product.form</field>
			<field name="model">product.product</field>
			<field name="inherit_id" ref="product.product_normal_form_view"/>
			<field name="arch" type="xml">
				<field name="barcode" position="before">
					<field name="url_product"/>
				</field>
				
			</field>
		</record>

		<record id="mobel.product_inherit_mobel_form_view2" model="ir.ui.view">
			<field name="name">mobel.inherit.product.product.form</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_only_form_view"/>
			<field name="arch" type="xml">
				<field name="barcode" position="before">
					<field name="url_product"/>
				</field>
				
			</field>
		</record>

</odoo>
