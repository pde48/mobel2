        <record id="seq_purchase_order_import" model="ir.sequence">
            <field name="name">Purchase Order Import</field>
            <field name="code">purchase.import</field>
            <field name="prefix">IMP-%(y)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_purchase_order_exception" model="ir.sequence">
